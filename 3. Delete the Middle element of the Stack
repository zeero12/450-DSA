class Solution
{
    public:
    //Function to delete middle element of a stack.
    void deleteMid(stack<int>&s, int sizeOfStack)
    {
        // code here..
        stack <int> st; 
        
         
        for(int i=0; i<ceil((sizeOfStack+1)/2) + 1; i++) {
            st.push(s.top());
            s.pop();
        }
        // Popping the middle element
        //st.pop();
        // Restoring the stack
        while(!st.empty()) {
            s.push(st.top());
            st.pop();
        }
  
    }
    
};
