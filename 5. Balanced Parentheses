bool eq(char a, char b)
{
    return (a== '(' and b==')' || a== '{' and b=='}' || a== '[' and b==']');
}

class Solution
{
    public:
    //Function to check if brackets are balanced or not.
    bool ispar(string s)
    {
       stack<char> st; 
      int n = s.size();

for(int i = 0; i < n; i++)
{    
     if(s[i] == '(' or s[i] == '{' or s[i] == '[')
         st.push(s[i]);
  
        
     else if(s[i] == ')'  or s[i] == '}'   or s[i] == ']') 
    {
 
       if(!st.empty() and eq(st.top(),s[i]))
                  st.pop();
        
       else
              return false; 
   }
           
    
         
}   

  if(st.empty())
       return true;
  else 
     return false;
 } // Your code here    }

};
