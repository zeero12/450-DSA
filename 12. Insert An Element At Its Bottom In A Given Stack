stack<int> pushAtBottom(stack<int>& myStack, int x) 
{
    stack<int> s; 

    
    while(myStack.size() != 0)
    { 
        s.push(myStack.top());
        myStack.pop();
     }
    
       myStack.push(x);
      while(s.size() != 0)
    { 
        myStack.push(s.top());
        s.pop();
      }   
    return myStack;
}
